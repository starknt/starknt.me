  "devDependencies": {
    "@antfu/eslint-config": "^0.25.2",
    "@iconify/json": "^2.1.86",
    "@testing-library/vue": "^6.6.1",
    "@types/diacritics": "^1.3.1",
    "@types/markdown-it": "^12.2.3",
    "@types/markdown-it-link-attributes": "^3.0.1",
    "@types/node": "^18.0.6",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vue/compiler-sfc": "^3.2.37",
    "diacritics": "^1.3.0",
